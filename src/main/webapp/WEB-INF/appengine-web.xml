<?xml version="1.0" encoding="UTF-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0"
xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
xsi:schemaLocation='http://kenai.com/projects/nbappengine/downloads/download/schema/appengine-web.xsd appengine-web.xsd'>
    <application>passcheck</application>
    <version>2</version>
    <ssl-enabled>true</ssl-enabled>
    <sessions-enabled>false</sessions-enabled>
    
    <threadsafe>true</threadsafe>
    <use-google-connector-j>true</use-google-connector-j>
    
    <system-properties>
        <property name="passcheck.pass_db" value="jdbc:google:mysql://" />
    </system-properties>
</appengine-web-app>
